<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_gray"
    android:orientation="vertical"
    android:padding="@dimen/dimen_10"
    tools:context="com.ortho.medicare.medicareortho.fragments.QualityFragment">

    <com.ortho.medicare.medicareortho.customviews.CustomTextView
        android:id="@+id/frg_quality_txt_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        app:font_type="bold" />

    <com.ortho.medicare.medicareortho.customviews.CustomTextView
        android:id="@+id/frg_quality_txt_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_05" />

    <com.github.barteksc.pdfviewer.PDFView
        android:id="@+id/frg_quality_pdf_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dimen_05" />

</LinearLayout>
